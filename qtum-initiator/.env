# chainlink node url
CHAINLINK_BASE_URL=http://chainlink-node:6688/
# Database full path
DATABASE_URL=postgresql://postgres:chainlink@postgres-server:5432/qtum_initiator?sslmode=disable&client_encoding=utf8&connect_timeout=5000
# Initiator host
INITIATOR_HOST=qtum-initiator
# Initiator name to register
INITIATOR_NAME=qtuminitiator
# Initiator service port
INITIATOR_PORT=30055
# The number of blocks to wait before triggering a job run
MIN_INCOMING_CONFIRMATIONS=0
# hostname of qtumnode
QTUM_HOST=
# port of qtum node RPC
QTUM_PORT=23889
# url of qtum node RPC
QTUM_URL=/# protocol of qtum node WS
QTUM_WS_PROTOCOL=ws
# port of qtum node WS
QTUM_WS_PORT=23889
# url of qtum node WS
QTUM_WS_URL=/websocket
