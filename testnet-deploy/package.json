{
	"name": "testnet-deploy",
	"version": "0.0.1",
	"description": "Deploy Oracle to QTUM Testnet",
	"scripts": {
		"test": "npx truffle test",
		"postinstall": "cd node_modules/qtumjs-eth/ && yarn && yarn build ; cd ../../ ",
		"deploy:oracle:qtum": "npx truffle migrate --f 2 --to 2 --network qtum",
		"deploy:consumer:qtum": "npx truffle migrate --f 3 --to 3 --network qtum"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"minimist": "^1.2.5",
		"ora": "^5.1.0",
		"qtumjs": "^1.9.3",
		"qtumjs-eth": "ceoimon/qtumjs-eth#master",
		"solc": "0.5.0"
	},
	"devDependencies": {}
}
